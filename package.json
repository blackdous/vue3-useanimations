{
  "name": "vue3-useanimations",
  "version": "0.0.30",
  "description": "vue3-useanimations is a collection of free animated open source icons for vue.js.",
  "main": "dist/vue3-UseAnimations.umd.cjs",
  "module": "dist/vue3-UseAnimations.js",
  "sideEffects": false,
  "browser": {
    "./dist/vue3-UseAnimations.umd.cjs": "./dist/vue3-UseAnimations.umd.cjs",
    "./dist/vue3-UseAnimations.js": "./dist/vue3-UseAnimations.js"
  },
  "unpkg": "dist/vue3-UseAnimations.umd.cjs",
  "jsdelivr": "dist/vue3-UseAnimations.umd.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && rollup -c build/rollup.config.js",
    "build:vite": "rimraf dist && vite build",
    "test:watch": "jest --testPathIgnorePatterns test/performance --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "npx eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "check-types": "tsc --noEmit",
    "check-types:watch": "npm run type-check -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackdous/vue3-useanimations.git"
  },
  "keywords": [
    "Vue3",
    "TypeScript",
    "useAnimations",
    "Icons",
    "lottie"
  ],
  "author": "@blockdous",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackdous/vue3-useanimations/issues"
  },
  "homepage": "https://github.com/blackdous/vue3-useanimations#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run test:all",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "core-js": "^3.30.2",
    "lottie-web": "^5.11.0",
    "rollup-plugin-copy": "^3.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.3.2",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.4.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.2"
  }
}
